[ignore]
.*/tests/.*
.*/dist/.*
.*/node_modules/jsonlint/test/.*

[include]

[libs]
.*/flow-typed/.*

[lints]

[options]
emoji=true
module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.json
module.file_ext=.css
module.file_ext=.scss
module.file_ext=.less
module.name_mapper.extension='css' -> 'empty/object'
module.name_mapper.extension='scss' -> 'empty/object'
module.name_mapper.extension='less' -> 'empty/object'
module.system.node.resolve_dirname=node_modules
module.system.node.resolve_dirname=src
module.name_mapper='^@app' ->'<PROJECT_ROOT>/src/app/App.jsx'
module.name_mapper='^@configureStore' -> '<PROJECT_ROOT>/src/shared/redux/configureStore.jsx'
module.name_mapper='^@routes' -> '<PROJECT_ROOT>/src/shared/routes.jsx'
module.name_mapper='^@components' ->'<PROJECT_ROOT>/src/app/components'
module.name_mapper='^@api' ->'<PROJECT_ROOT>/src/app/features/api'
module.name_mapper='^@context' ->'<PROJECT_ROOT>/src/app/features/context'
module.name_mapper='^@actions' ->'<PROJECT_ROOT>/src/app/features/actions'
module.name_mapper='^@reducers' ->'<PROJECT_ROOT>/src/app/features/reducers'
module.name_mapper='^@views' ->'<PROJECT_ROOT>/src/app/views'
module.name_mapper='^@sharedComponents' -> '<PROJECT_ROOT>/src/shared/components'
module.name_mapper='^@sharedReducers' ->'<PROJECT_ROOT>/src/shared/reducers'
module.name_mapper='^@sharedUtils' ->'<PROJECT_ROOT>/src/shared/utils'
module.name_mapper='^@sharedStyles' ->'<PROJECT_ROOT>/src/shared/styles'
module.name_mapper='^@server' ->'<PROJECT_ROOT>/src/server'

[strict]

[version]
>=0.72.0
